    <script>
      $(document).ready(() => {
        // const userRoll = getUserData();
        axios
          .get("/auth/user")
          .then((res) => {
            $("#userName").html(res.data.user_name);
            $("#userFullName").html(res.data.user_name);
            $("#userEmail").html(res.data.email);
            dashBoardController(res.data.roll);
            return res.data.roll;
          })
          .catch(function (error) {
            console.error(error.message);
          });
        const dashBoardController = (roll) => {
          if (roll == "cre") {
            $("#salesRatio").hide();
          }
        };

        const createUserAllowTable = () => {
          axios.get("/user/pandingUsers").then((res) => {
            const data = res.data.userList;
            if (data.length > 0) {
              const tableHTML = generateTable(data);
              $("#pandingList").html(tableHTML);
            } else {
              $("#pandingList").html(
                '<h1 class="d-inline m-40 text-muted">No Pandding Request</h1>'
              );
            }
          });
        };

        createUserAllowTable();
        // For create Inprocess data Table
        function generateTable(data) {
          let tableHTML =
            "<table class='table table-striped table-bordered display nowrap' style='width:100%'><thead><tr><th>Name</th><th>Roll</th><th>Email</th><th>Date</th><th>Action</th></tr></thead><tbody>";
          data.forEach((row) => {
            tableHTML += `<tr>
            <td>${row.user_name}</td>
            <td>${row.roll}</td>
            <td>${row.email}</td>
            <td>${row.create_date}</td>
            <td>
              <a class="allow">
                    <i class="ti-check"></i>
                    <p class="d-none">${row.user_id}</p>
              </a>
              <a class="text-danger pl-3 deny">
                    <i class="ti-close"></i>
                    <p class="d-none">${row.user_id}</p>
              </a>
            </td>
        </tr>`;
          });
          tableHTML += "</tbody></table>";
          return tableHTML;
        }

        $(document).on("click", ".deny", (event) => {
          event.preventDefault();
          const id = $(event.currentTarget).find("p").text();
          Swal.fire({
            title: "Are you sure?",
            text: "You won't be able to revert this!",
            icon: "warning",
            showCancelButton: true,
            confirmButtonColor: "#3085d6",
            cancelButtonColor: "#d33",
            confirmButtonText: "Yes, delete it!",
          }).then((result) => {
            if (result.isConfirmed) {
              axios
                .patch(`/user/denyUser/${id}`)
                .then((res) => {
                  if (res.status == 200) {
                    Swal.fire({
                      title: "The Internet?",
                      text: "That thing is still around?",
                      icon: "success",
                    }).then(() => {
                      createUserAllowTable();
                    });
                  }
                })
                .catch((err) => {
                  console.error(err);
                });
            }
          });
        });

        $(document).on("click", ".allow", (event) => {
          event.preventDefault();
          const id = $(event.currentTarget).find("p").text();
          Swal.fire({
            title: "Are you sure?",
            text: "You won't be able to revert this!",
            icon: "warning",
            showCancelButton: true,
            confirmButtonColor: "#3085d6",
            cancelButtonColor: "#d33",
            confirmButtonText: "Yes, delete it!",
          }).then((result) => {
            if (result.isConfirmed) {
              axios
                .patch(`/user/allowUser/${id}`)
                .then((res) => {
                  if (res.status == 200) {
                    Swal.fire({
                      title: "The Internet?",
                      text: "That thing is still around?",
                      icon: "success",
                    }).then(() => {
                      createUserAllowTable();
                    });
                  }
                })
                .catch((err) => {
                  console.error(err);
                });
            }
          });
        });
      });
    </script>
